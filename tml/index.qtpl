Note: All text outside function is considered comments

{% func Compile(alerts []string) %}
<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Alarms</title>
  </head>
  <body>
    <h1>Alarms</h1>
    <ul>
      {% if len(alerts) == 0 %}
        <p>No alarms at this time.</p>
      {% endif %}
      {% for i, alert := range alerts %}
        <li>{%d i %} -- {%s alert %}</li>
      {% endfor %}
    </ul>
  </body>
</html>
{% endfunc %}
